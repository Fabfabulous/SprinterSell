<div class="container">
  <div class="home-left">
    <div class="agenda">
      <%= render partial:"meetings/agenda", locals: { meetings: @meetings, companies: @companies_suggestion} %>
    </div>
    <%# style="display:none;" %>
    <div class="suggestion">
      <h2>Suggestion</h2>
    </div>
  </div>
  <div class="home-right">
    <div style="width: 100%; height: 100%; "
      data-controller="map"
      data-map-target="map"
      data-map-markers-value="<%= @markers.to_json %>"
      data-map-markers-prospect-value="<%= @markers_prospect.to_json %>"
      data-map-api-key-value="<%= ENV['MAPBOX_API_KEY'] %>">
    </div>
  </div>
</div>
