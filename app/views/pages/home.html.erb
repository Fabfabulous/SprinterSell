<div class="container">
  <div class="home-left">
    <div class="agenda">
      <%= render "meetings/agenda", meetings: @meetings %>
    </div>
    <%# style="display:none;" %>
    <div class="suggestion">
      <%= render "pages/company-list", companies: @companies_suggestion  %>
    </div>
  </div>
  <div class="home-right">
    <div style="width: 100%; height: 100%; "
      data-controller="map"
      data-map-markers-value="<%= @markers.to_json %>"
      data-map-api-key-value="<%= ENV['MAPBOX_API_KEY'] %>">
    </div>
  </div>
</div>
